{
  "manifest_version": 4,
  "name": "TRPG Character Maker",
  "version": "1.0.2",
  "permissions": [ "tabs" ],
  "description": "코코포리아 및 구글 시트와 연계할 수 있는 TRPG 캐릭터 생성기",
  "icons": {
    "16": "16image.png",
    "48": "48image.png",
    "128": "128image.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "16image.png",
    "default_title": "TRPG Character Maker"
  }
}